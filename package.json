{
  "name": "dawww",
  "version": "0.0.0-development",
  "description": "Simple backend for a step sequencer",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --ignore test.js",
    "trybuild": "babel src --ignore test.js",
    "clean": "rimraf dist",
    "lint": "prettier src --write \"{examples,src}/**/*.js\"",
    "prebuild": "npm run clean -s",
    "prepush": "npm run lint && npm run testonce && npm run trybuild",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "parcel ./examples/index.html",
    "test": "ava --watch",
    "testonce": "nyc ava"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/register": "7.4.4",
    "ava": "1.4.1",
    "browser-env": "3.2.6",
    "codecov": "3.3.0",
    "husky": "0.14.3",
    "nyc": "14.1.0",
    "parcel": "1.12.3",
    "prettier": "1.17.0",
    "rimraf": "2.6.3",
    "semantic-release": "15.9.12",
    "sinon": "7.3.2",
    "superfine": "6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nickjohnson-dev/dawww.git"
  },
  "ava": {
    "require": [
      "@babel/register",
      "./test/helpers/setup-browser-env.js"
    ]
  },
  "babel": {
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "./configs/.eslintrc.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nickjohnson-dev/dawww/issues"
  },
  "homepage": "https://github.com/nickjohnson-dev/dawww#readme",
  "dependencies": {
    "deep-diff": "0.3.8",
    "event-emitter": "0.3.5",
    "lodash": "4.17.20",
    "shortid": "2.2.14",
    "tone": "13.4.9"
  }
}
