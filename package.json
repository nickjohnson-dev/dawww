{
  "name": "dawww",
  "version": "0.0.0-development",
  "description": "Simple backend for a step sequencer",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --ignore test.js",
    "trybuild": "babel src --ignore test.js",
    "clean": "rimraf dist",
    "cm": "git-cz",
    "cmretry": "git-cz --retry",
    "lint": "eslint src",
    "prebuild": "npm run clean -s",
    "precommit": "npm run lint && npm run testonce && npm run trybuild",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "webpack-dev-server --config ./examples/configs/webpack-dev.config.js --port 8082 --open",
    "test": "ava --watch",
    "testonce": "nyc ava"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "circular-dependency-plugin": "4.0.0",
    "clean-webpack-plugin": "0.1.16",
    "codecov": "2.3.0",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "eslint": "4.5.0",
    "eslint-config-airbnb-base": "11.3.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-lodash-fp": "2.1.3",
    "html-webpack-plugin": "2.30.1",
    "html-webpack-template": "5.6.0",
    "husky": "0.14.3",
    "nyc": "11.2.1",
    "picodom": "0.1.0",
    "rimraf": "2.6.1",
    "semantic-release": "^7.0.2",
    "sinon": "4.0.1",
    "webpack": "3.5.5",
    "webpack-dev-server": "2.7.1",
    "webpack-merge": "4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nickjohnson-dev/dawww.git"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./configs/.eslintrc.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nickjohnson-dev/dawww/issues"
  },
  "homepage": "https://github.com/nickjohnson-dev/dawww#readme",
  "dependencies": {
    "deep-diff": "0.3.8",
    "event-emitter": "0.3.5",
    "lodash": "4.17.4",
    "tone": "0.11.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
