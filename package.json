{
  "name": "dawww",
  "version": "0.0.0-development",
  "description": "Simple backend for a step sequencer",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist --ignore test.js",
    "trybuild": "babel src --ignore test.js",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "prebuild": "npm run clean -s",
    "prepush": "npm run lint && npm run testonce && npm run trybuild",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "start": "parcel ./examples/index.html",
    "test": "ava --watch",
    "testonce": "nyc ava"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "browser-env": "3.2.1",
    "codecov": "2.3.0",
    "eslint": "4.5.0",
    "eslint-config-airbnb-base": "11.3.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-lodash-fp": "2.1.3",
    "husky": "0.14.3",
    "nyc": "11.2.1",
    "parcel": "1.12.3",
    "picodom": "0.1.0",
    "rimraf": "2.6.1",
    "semantic-release": "15.9.12",
    "sinon": "4.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nickjohnson-dev/dawww.git"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/setup-browser-env.js"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./configs/.eslintrc.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nickjohnson-dev/dawww/issues"
  },
  "homepage": "https://github.com/nickjohnson-dev/dawww#readme",
  "dependencies": {
    "deep-diff": "0.3.8",
    "event-emitter": "0.3.5",
    "lodash": "4.17.11",
    "shortid": "2.2.8",
    "tone": "0.11.11"
  }
}
